        <key>VERSION</key>
        <string>1.3.1</string>
        <key>NAME</key>
        <string>ButeLoudnessSuiteSurround</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Arguments</key>
            <dict>
		<key>url</key>
		<string>http://www.signumaudio.com/distributables/%NAME%Mac1.3.1.pkg.zip</string>
                <key>filename</key>
                <string>%NAME%-%VERSION%.pkg.zip</string>
            </dict>
            <key>Processor</key>
            <string>URLDownloader</string>
        </dict>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>filename</key>
                <string>%NAME%-%VERSION%.pkg.zip</string>
            </dict>
            <key>Processor</key>
            <string>Unarchiver</string>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
