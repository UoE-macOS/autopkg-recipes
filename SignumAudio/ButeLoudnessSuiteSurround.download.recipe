<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Download recipe for Bute Loudness Suite - Surround</string>
    <key>Identifier</key>
    <string>com.github.autopkg.download.bute_loudness_surround</string>
    <key>Input</key>
